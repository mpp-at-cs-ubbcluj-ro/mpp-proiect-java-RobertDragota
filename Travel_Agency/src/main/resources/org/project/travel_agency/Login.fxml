<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #404040;" xmlns="http://javafx.com/javafx/" xmlns:fx="http://javafx.com/fxml/" fx:controller="org.project.travel_agency.Controllers.Login_Controller">
   <children>
      <Button fx:id="login_button" layoutX="219.0" layoutY="245.0" mnemonicParsing="false" onAction="#handleLogin" prefHeight="41.0" prefWidth="162.0" stylesheets="@login_style.css" text="Login" />
      <TextField fx:id="UserName" layoutX="194.0" layoutY="128.0" prefHeight="26.0" prefWidth="214.0" promptText="username" styleClass="rounded-textfield" stylesheets="@login_style.css" />
      <Text fill="#b1d728" layoutX="147.0" layoutY="72.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Travel_App" textAlignment="CENTER" wrappingWidth="306.0">
         <font>
            <Font name="Lato Bold" size="40.0" />
         </font>
      </Text>
      <Button fx:id="register_button" layoutX="424.0" layoutY="345.0" mnemonicParsing="false" onAction="#handleRegister" prefHeight="41.0" prefWidth="162.0" stylesheets="@login_style.css" text="Register" />
      <PasswordField fx:id="UserPassword" layoutX="194.0" layoutY="181.0" prefHeight="38.0" prefWidth="214.0" promptText="password" styleClass="rounded-textfield" stylesheets="@login_style.css" />
   </children>
</AnchorPane>
